{
  "apphosting": {
    "backendId": "lin-llc",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "emulators": {
    "auth": {},
    "firestore": {
      "rules": "firestore.rules"
    },
    "storage": {
      "rules": "storage.rules"
    },
    "ui": {
      "enabled": true
    }
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "lin-llc.firebasestorage.app"
  }
}
